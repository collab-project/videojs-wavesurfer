{
  "name": "videojs-wavesurfer-pages",
  "private": true,
  "version": "1.0.0",
  "description": "videojs-wavesurfer github pages",
  "homepage": "https://collab-project.github.io/videojs-wavesurfer",
  "author": "Collab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/collab-project/videojs-wavesurfer/issues"
  },
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/collab-project/videojs-wavesurfer.git"
  },
  "dependencies": {
    "video.js": "^6.8.0",
    "videojs-wavesurfer": "^2.3.1",
    "webrtc-adapter": "^6.1.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "connect": "^3.6.6",
    "copy-dir": "^0.3.0",
    "cpx": "^1.5.0",
    "del": "^3.0.0",
    "download-tarball": "^1.1.0",
    "htmlhint": "^0.9.13",
    "in-publish": "^2.0.0",
    "mv": "^2.1.1",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.2",
    "portscanner": "^2.1.1",
    "serve-static": "^1.13.2",
    "zip-dir": "^1.0.2"
  },
  "scripts": {
    "lint": "npm-run-all lint:*",
    "lint:html": "htmlhint examples index.html",
    "start": "npm-run-all -p start:* watch",
    "start:serve": "babel-node --presets=env scripts/server.js",
    "update": "npm-run-all -s update:*",
    "update:videojs-wavesurfer": "cpx -v 'node_modules/videojs-wavesurfer/dist/**/*' dist/",
    "update:wavesurfer": "cpx -v 'node_modules/wavesurfer.js/dist/wavesurfer.min.*' dist/",
    "update:wavesurfer-microphone": "cpx -v 'node_modules/wavesurfer.js/dist/plugin/wavesurfer.microphone.min.*' dist/",
    "release:zip": "node scripts/release-zip.js",
    "test": "echo No tests yet!",
    "watch": "npm-run-all -p watch:lint",
    "watch:lint": "nodemon --watch examples --exec npm run lint",
    "prepublish": "not-in-install && npm run build || in-install"
  }
}
